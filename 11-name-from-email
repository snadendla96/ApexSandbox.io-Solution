public String nameFromEmail(String email) {
 String fullName=email.substringBefore('@');
   String firstname=fullName.substringBefore('.');
   String lastname=fullName.substringAfter('.');
   String finalName=firstname+' '+lastname;
   String finalString;
    if(!finalName.isAllLowerCase()){
         finalString=firstname.capitalize()+' '+lastname.capitalize();
    }
    return finalString;
   
}