public String aOrAn(String word) {
    //code here
    String statment;
    String lowecaseword=word.tolowerCase();
    if(lowecaseword.startsWith('a') || lowecaseword.startsWith('e')
       lowecaseword.startsWith('i') || lowecaseword.startsWith('o') ||
       lowecaseword.startsWith('u')){
       statment='an'+word;
    }else{
        statment='a'+word;
    }
    return statment;
}