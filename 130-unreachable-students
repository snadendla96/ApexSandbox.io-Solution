public List<apxio__Student__c> selectUnreachableStudents() {
    // code here
    List<apxio__Student__c> unreachableStudents=[SELECT ID,Name as Student Name,apxio__Phone__c,apxio__Email__c,apxio__Active__c from apxio__Student__c 
          where apxio__Active__c=true and  apxio__Email__c=Null And apxio__Phone__c=Null order by Name asc];

     return unreachableStudents;

}